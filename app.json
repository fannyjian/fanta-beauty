{
  "expo": {
    "name": "fanta-beauty",
    "slug": "fanta-beauty",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/fantalogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/fantabeauty.png",
      "resizeMode": "contain",
      "backgroundColor": "#F3E9F9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "sourceExts": ["cjs"]
    },
    "privacy" : "public"
  }
}
